{
  "logger": {
    "path": "",
    "filename": "access.log",
    "level": "INFO",
    "rotation": "1 days",
    "retention": "1 months",
    "format": "<green>{time:YYYY-MM-DD HH:mm:ss.SSS}</green> - <level>{level}</level> - (request-id: {extra[request_id]}) - <cyan>{name}</cyan>:<cyan>{function}</cyan> - <level>{message}</level>"
  }
}